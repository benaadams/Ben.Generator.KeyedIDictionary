<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <ILLinkTrimAssembly>false</ILLinkTrimAssembly>
    <NoWarn>$(NoWarn);CS3001</NoWarn>
    <Version>0.0.1</Version>
    <RepositoryUrl>https://github.com/benaadams/Ben.Generator.KeyedIDictionary</RepositoryUrl>
    <PackageProjectUrl>https://github.com/benaadams/Ben.Generator.KeyedIDictionary</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.9.0" />
  </ItemGroup>
</Project>
